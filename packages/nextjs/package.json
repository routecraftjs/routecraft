{
  "name": "@routecraftjs/nextjs",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/mod.cjs",
  "module": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js",
      "require": "./dist/mod.cjs"
    },
    "./plugin": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js",
      "require": "./dist/mod.cjs"
    },
    "./api": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js",
      "require": "./dist/mod.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/mod.ts --format esm,cjs --dts",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@routecraftjs/routecraft": "workspace:*",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "dotenv": "^16.4.5",
    "minimatch": "^10.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "https://github.com/devoptix-labs/routecraft",
  "bin": {}
}
